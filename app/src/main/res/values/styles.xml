<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorFrogGreen</item>
        <item name="colorPrimaryDark">@color/colorDarkGreen</item>
        <item name="colorAccent">@color/colorWhite</item>
        <item name="colorControlNormal">@color/colorWhite</item>
        <item name="android:windowNoTitle">true</item>

    </style>
    <style name="ActionBar" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorFrogGreen</item>
        <item name="colorPrimaryDark">@color/colorDarkGreen</item>
        <item name="colorAccent">@color/colorDarkBlue</item>
        <item name="colorControlNormal">@color/colorBlue</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="Dashboard" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorFrogGreen</item>
        <item name="colorPrimaryDark">@color/colorDarkGreen</item>
        <item name="colorAccent">@color/colorDarkBlue</item>
        <item name="colorControlNormal">@color/colorBlue</item>
        <item name="android:windowNoTitle">true</item>
    </style>
</resources>
