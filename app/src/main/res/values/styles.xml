<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="colorControlNormal">#FFF</item>
        <item name="android:windowNoTitle">true</item>

    </style>
    <style name="ActionBar" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorPrimaryActionBar</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDarkActionBar</item>
        <item name="colorAccent">@color/colorAccentActionBar</item>
        <item name="colorControlNormal">@color/colorControlNormalActionBar</item>
        <item name="android:windowNoTitle">true</item>
    </style>
</resources>
